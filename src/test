#!/bin/bash
echo "Starting MRS program"
echo "===================="

sh compile

if [ $1 = "Robot" ]
then
    echo "Running java -cp .:./json-simple-1.1.1.jar .:./../bin rgraph.Test${1}"
    java -cp .:./json-simple-1.1.1.jar -cp .:./../bin rgraph.Test$1
else
    echo "Running java -cp .:./json-simple-1.1.1.jar .:./../bin ralgorithm.Test${1}"
    java -cp .:./json-simple-1.1.1.jar -cp .:./../bin ralgorithm.Test$1
fi
